	"border-top: $1;": { "prefix": "bd-t", "body": ["border-top: ${1:1px} ${2:solid} ${3:#ddd};"], "description": "" },
	"border-left: $1;": { "prefix": "bd-l", "body": ["border-left: ${1:1px} ${2:solid} ${3:#ddd};"], "description": "" },
