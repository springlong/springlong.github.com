	"box-shadow: $1;": { "prefix": "sd", "body": ["box-shadow: ${1:水平偏移量} ${2:垂直偏移量} ${3:模糊程度} ${4:阴影颜色};"], "description": "" },
	// 背景
	"background: $1": { "prefix": "bk", "body": ["background: url($1) ${2:no-repeat} ${3:0 0};"], "description": "背景设置" },
	"background-color: $1": { "prefix": "bk-c", "body": ["background-color: $1;"], "description": "背景颜色" },
	"background-image: $1": { "prefix": "bk-i", "body": ["background-image: url($1);"], "description": "背景图" },
	"background-attachment: fixed": { "prefix": "bk-fix", "body": ["background-attachment: fixed;"], "description": "背景图-固定" },
	"background-attachment: scroll": { "prefix": "bk-scr", "body": ["background-attachment: scroll;"], "description": "背景图-可滚动" },
	"background-position: $1": { "prefix": "bk-p", "body": ["background-position: $1;"], "description": "背景图-显示位置" },
	"background-repeat: $1": { "prefix": "bk-r", "body": ["background-repeat: $1;"], "description": "背景图-平铺方式" },
	"background-size: $1": { "prefix": "bk-s", "body": ["background-size: ${1:<length>|<percentage>|auto|cover|contain};"], "description": "背景图-大小" },
	"background-size: cover": { "prefix": "bk-s", "body": ["background-size: cover;"], "description": "背景图-等比缩放完全覆盖容器，有可能超出容器" },
	"background-size: contain": { "prefix": "bk-s", "body": ["background-size: contain;"], "description": "背景图-等比缩放到宽度或高度与容器的宽度或高度相等，始终被包含在容器内" },
