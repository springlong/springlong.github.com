	"max-width: $1;": { "prefix": "maxwd", "body": ["max-width: $1;"], "description": "" },
	"height: $1;": { "prefix": "ht", "body": ["height: $1;"], "description": "" },
	"min-height: $1;": { "prefix": "mht", "body": ["min-height: $1;"], "description": "" },
	"max-height: $1;": { "prefix": "maxht", "body": ["max-height: $1;"], "description": "" },
	"padding: $1;": { "prefix": "pd", "body": ["padding: $1;"], "description": "" },
	"padding-left: $1;": { "prefix": "pd-l", "body": ["padding-left: $1;"], "description": "" },
