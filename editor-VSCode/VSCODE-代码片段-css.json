{
	// Place your snippets for css here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"position: absolute;": { "prefix": "abs", "body": ["position: absolute;"], "description": "" },
	"position: fixed;": { "prefix": "fix", "body": ["position: fixed;"], "description": "" },
	"position: relative;": { "prefix": "rel", "body": ["position: relative;"], "description": "" },
	"position: static;": { "prefix": "sta", "body": ["position: static;"], "description": "" },
	"left: $1;": { "prefix": "lf", "body": ["left: $1;"], "description": "" },
	"right: $1;": { "prefix": "ri", "body": ["right: $1;"], "description": "" },
	"top: $1;": { "prefix": "tp", "body": ["top: $1;"], "description": "" },
	"bottom: $1;": { "prefix": "bt", "body": ["bottom: $1;"], "description": "" },
	"z-index: $1;": { "prefix": "zin", "body": ["z-index: $1;"], "description": "" },
