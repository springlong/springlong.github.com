	"opacity: $1;": { "prefix": "op", "body": ["opacity: $1;"], "description": ""},
	// 代码片段
	"多行省略号": {
		"prefix": "ellip:lines",
		"body": [
			"display: -webkit-box;",
			"-webkit-box-orient: vertical;",
			"-webkit-line-clamp: ${1:2};",
			"overflow: hidden;"
		],
		"description": ""
	},
	"单行省略号": {
		"prefix": "ellip:single",
		"body": [
			"overflow: hidden;",
			"text-overflow:ellipsis;",
			"white-space: nowrap;"
		],
		"description": ""
	},
