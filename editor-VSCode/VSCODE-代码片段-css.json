	"text-overflow: clip;": { "prefix": "to-cl", "body": ["text-overflow: clip;"], "description": "" },
	"text-overflow: ellipsis;": { "prefix": "to-el", "body": ["text-overflow: ellipsis;"], "description": "" },
	"text-shadow: $1;": { "prefix": "tsha", "body": ["text-shadow: ${1:水平偏移量} ${2:垂直偏移量} ${3:模糊程度} ${4:阴影颜色};"], "description": "" },
	"color: $1;": { "prefix": "co", "body": ["color: $1;"], "description": "" },
