	"margin-right: $1;": { "prefix": "mg-r", "body": ["margin-right: $1;"], "description": "" },
	"margin-top: $1;": { "prefix": "mg-t", "body": ["margin-top: $1;"], "description": "" },
	"margin-bottom: $1;": { "prefix": "mg-b", "body": ["margin-bottom: $1;"], "description": "" },
