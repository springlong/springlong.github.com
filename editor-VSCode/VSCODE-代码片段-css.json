	"background-origin: padding": { "prefix": "bk-o", "body": ["background-origin: padding;"], "description": "背景图-从什么位置开始显示" },
	"background-origin: content": { "prefix": "bk-o", "body": ["background-origin: content;"], "description": "背景图-从什么位置开始显示" },
