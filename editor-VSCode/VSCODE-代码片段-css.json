	"display: inline;": { "prefix": "il", "body": ["display: inline;"], "description": "" },
	"display: flex;": { "prefix": "flx", "body": ["display: flex;"], "description": "" },
	"display: none;": { "prefix": "non", "body": ["display: none;"], "description": "" },
	// float
